        <t t-jquery="div.o_list_buttons" t-operation="append">
            <t t-if = "widget.modelName == 'x_this_is_tour_123'">
                <button type="button" class="btn btn-primary cortana__export_button__button">
                    Export
                </button>
                <button type="button" class="btn btn-primary cortana__export_a_to_d_button__button">
                    日程表
                </button>
                <button type="button" class="btn btn-primary cortana__export_e_to_f_button__button">
